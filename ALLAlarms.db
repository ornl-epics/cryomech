record(calc, "$(PV)1") {
  field(DESC, "All Alarms Anded")
  field(INPA, "BL15:SE:CM:1:Status CP MS")
  field(INPB, "BL15:SE:CM:1:OilTempAlarm CP MS")
  field(INPC, "BL15:SE:CM:1:INCoolantTempAlarm CP MS")
  field(INPD, "BL15:SE:CM:1:OUTCoolantTempAlarm CP MS")
  field(INPE, "BL15:SE:CM:1:LowPressSideAlarm CP MS")
  field(INPE, "BL15:SE:CM:1:HighPressSideAlarm CP MS")
  field(INPF, "BL15:SE:CM:1:CurrentAlarm CP MS")
  field(CALC, "(A+B+C+D+E+F)>0?1:0")
  field(HIHI, "1")
  field(HHSV, "MAJOR")
  info(archive, "Monitor, 00:00:01, VAL")
}

